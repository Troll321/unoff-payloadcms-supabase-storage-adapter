    "version": "1.0.3",
    "description": "An unofficial implementation for payloadcms supabase storage adapter",
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc"
    },
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
