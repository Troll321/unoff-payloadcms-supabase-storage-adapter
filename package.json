    "dependencies": {
        "@payloadcms/plugin-cloud-storage": "^3.74.0",
        "@supabase/supabase-js": "^2.94.0"
    },
    "peerDependencies": {
        "payload": "^3.74.0"
    },
    "devDependencies": {
        "@types/node": "^25.2.0",
        "payload": "^3.74.0",
